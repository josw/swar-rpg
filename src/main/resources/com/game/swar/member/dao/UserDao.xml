<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.game.swar.member.dao.UserDao">

    <select id="getUser" resultType="com.game.swar.member.model.User" parameterType="java.lang.String">
